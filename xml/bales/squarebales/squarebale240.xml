    </baleMeshes>

    <variations>
        <variation id="DEFAULT"/>
        <variation id="TWINE">
            <netWrapDiffuse filename="$data/objects/bales/squarebales/squarebale240/netWrap_diffuse.dds"/>
            <netWrapNormal  filename="$data/objects/bales/squarebales/squarebale240/netWrap_normal.dds"/>
        </variation>
    </variations>

    <fillTypes>
        <fillType name="GRASS_WINDROW" capacity="6500" mass="100" forceAcceleration="7" supportsWrapping="true" materialName="wrapBale" alphaMaterialName="wrapBaleAlpha">
