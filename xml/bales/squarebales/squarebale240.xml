            <normal     filename="$data/objects/bales/squarebales/squarebale240/fillTypes/shared_normal.dds"/>
            <alpha      filename="$data/objects/bales/squarebales/squarebale240/bale_alpha.dds"/>
            <baleNormal filename="$data/shared/default_normal.dds"/>
            <fermenting outputFillType="silage" requiresWrapping="true" time="1"/>
        </fillType>
        <fillType name="DRYGRASS_WINDROW" capacity="8000" mass="100" forceAcceleration="7" supportsWrapping="false" materialName="wrapBale" alphaMaterialName="wrapBaleAlpha">
            <diffuse    filename="$data/objects/bales/squarebales/squarebale240/fillTypes/hay_diffuse.dds"/>
            <normal     filename="$data/objects/bales/squarebales/squarebale240/fillTypes/shared_normal.dds"/>
            <alpha      filename="$data/objects/bales/squarebales/squarebale240/bale_alpha.dds"/>
            <baleNormal filename="$data/shared/default_normal.dds"/>
        </fillType>
        <fillType name="STRAW" capacity="9000" mass="100" forceAcceleration="7" supportsWrapping="false" materialName="wrapBale" alphaMaterialName="wrapBaleAlpha">
            <diffuse    filename="$data/objects/bales/squarebales/squarebale240/fillTypes/straw_diffuse.dds"/>
            <normal     filename="$data/objects/bales/squarebales/squarebale240/fillTypes/shared_normal.dds"/>
            <alpha      filename="$data/objects/bales/squarebales/squarebale240/bale_alpha.dds"/>
            <baleNormal filename="$data/shared/default_normal.dds"/>
        </fillType>
        <fillType name="SILAGE" capacity="6500" mass="100" forceAcceleration="7" supportsWrapping="false" materialName="wrapBale" alphaMaterialName="wrapBaleAlpha">
            <diffuse    filename="$data/objects/bales/squarebales/squarebale240/fillTypes/silage_diffuse.dds"/>
            <normal     filename="$data/objects/bales/squarebales/squarebale240/fillTypes/shared_normal.dds"/>
            <alpha      filename="$data/objects/bales/squarebales/squarebale240/bale_alpha.dds"/>
            <baleNormal filename="$data/shared/default_normal.dds"/>
        </fillType>
    </fillTypes>
